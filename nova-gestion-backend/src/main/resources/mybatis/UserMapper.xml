<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nova.gestion.mappers.UserMapper">

    <select id="getUser" parameterType="Long" resultType="nova.gestion.model.User">
        SELECT * FROM `nova_gestion`.`user`
        WHERE id_user = #{user};
    </select>

</mapper>