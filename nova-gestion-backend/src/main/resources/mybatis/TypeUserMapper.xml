<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nova.gestion.mappers.TypeUserMapper">

    <resultMap id="TypeUserMap" type="nova.gestion.model.TypeUser">

    </resultMap>

    <select id="getTypeUser" parameterType="Long" resultType="nova.gestion.model.TypeUser">
        SELECT * FROM `nova_gestion`.`type_user`
        WHERE id_type_user = #{idUserType};
    </select>

</mapper>